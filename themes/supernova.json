{
  "name": "Supernova",
  "tokenColors": [
    {
      "settings": {
        "foreground": "#a1a4b5",
        "background": "#1b1d28"
      }
    },
    {
      "name": "Comments, Vendored Properties",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "support.type.vendored"
      ],
      "settings": {
        "foreground": "#5d6177"
      }
    },
    {
      "name": "Keywords, Storage, Primitive Types",
      "scope": [
        "keyword",
        "variable.language",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#9574bf"
      }
    },
    {
      "name": "Functions, Special Methods, Tag braces, IDs, Embeds",
      "scope": [
        "keyword.other.special-method",
        "support.function",
        "variable.function",
        "entity.name.function",
        "entity.other.attribute-name.id",
        "punctuation.definition.tag",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#5d95cc"
      }
    },
    {
      "name": "Classes, Annotations, Attribute Names",
      "scope": [
        "storage.type.annotation",
        "entity.other.attribute-name",
        "support.class",
        "support.type",
        "support.constant",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#c6a165"
      }
    },
    {
      "name": "Numbers, Characters, Constants",
      "scope": [
        "keyword.other.unit",
        "constant.numeric",
        "constant.character",
        "constant.language"
      ],
      "settings": {
        "foreground": "#c77d5f"
      }
    },
    {
      "name": "Strings, Packages, Namespaces",
      "scope": [
        "string",
        "entity.name.type.namespace",
        "storage.modifier.import",
        "storage.modifier.package"
      ],
      "settings": {
        "foreground": "#6ea066"
      }
    },
    {
      "name": "Constant Variables, Other Variables",
      "scope": [
        "variable.other",
        "variable.parameter",
        "support.other.variable",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#c75d7a"
      }
    },
    {
      "name": "User Defined Variables",
      "scope": [
        "variable.other.readwrite",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#a1a4b5"
      }
    },
    {
      "name": "Operators, Escape Chars, Regex, Colors",
      "scope": [
        "keyword.operator",
        "string.regexp",
        "constant.character.escape",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#55a8b5"
      }
    },
    {
      "name": "Tags, JSON Property Names",
      "scope": [
        "entity.name.tag",
        "source.json support.type.property-name"
      ],
      "settings": {
        "foreground": "#9574bf"
      }
    },
    {
      "name": "Blockquotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#5d6177"
      }
    },
    {
      "name": "Link Titles, Link Descriptions",
      "scope": [
        "string.other.link.description.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#5d95cc"
      }
    },
    {
      "name": "URLs",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#c75d7a"
      }
    },
    {
      "name": "Headings",
      "scope": [
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#6ea066"
      }
    },
    {
      "name": "Inlined Text, Fenced code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#55a8b5"
      }
    },
    {
      "name": "Deleted Items",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#c75d7a"
      }
    },
    {
      "name": "Added Items",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#6ea066"
      }
    },
    {
      "name": "Modified Items",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#c6a165"
      }
    },
    {
      "name": "Lists",
      "scope": [
        "beginning.punctuation.definition.list"
      ],
      "settings": {
        "foreground": "#c77d5f"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#9574bf"
      }
    },
    {
      "name": "Italics",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#c6a165"
      }
    },
    {
      "name": "CSS -- Property Names",
      "scope": [
        "source.css support.type.property-name"
      ],
      "settings": {
        "foreground": "#a1a4b5"
      }
    },
    {
      "name": "CSS - Constants",
      "scope": [
        "source.css support.constant"
      ],
      "settings": {
        "foreground": "#c77d5f"
      }
    },
    {
      "name": "SCSS -- Variables",
      "scope": [
        "variable.scss"
      ],
      "settings": {
        "foreground": "#c75d7a"
      }
    },
    {
      "name": "PHP -- Documentation",
      "scope": [
        "keyword.other.phpdoc"
      ],
      "settings": {
        "foreground": "#55a8b5"
      }
    }
  ],
